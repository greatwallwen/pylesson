   "source": [
    "from bokeh.plotting import figure, output_notebook, show\n",
    "# 输出到电脑屏幕上\n",
    "output_notebook()\n",
    "fig_obj = figure(plot_width=400, plot_height=400)\n",
    "# 添加矩形框，标有大小、颜色和alpha值\n",
    "fig_obj.square([2, 5, 6, 4], [2, 3, 2, 1], size=20, color=\"navy\")\n",
    "# 在默认的浏览器中显示图表\n",
    "show(fig_obj)"
   ]
